<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="472.0" prefWidth="624.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dictionary.textTranslateController">
   <children>
      <TextArea fx:id="txtInput" layoutX="14.0" layoutY="125.0" prefHeight="305.0" prefWidth="232.0" promptText="Text Input" />
      <Button fx:id="btnConvert" layoutX="279.0" layoutY="125.0" mnemonicParsing="false" onAction="#convertAction" text="Convert" />
      <TextArea fx:id="txtOutput" editable="false" layoutX="376.0" layoutY="125.0" prefHeight="305.0" prefWidth="232.0" promptText="Text Output" />
      <ComboBox fx:id="fromLang" layoutX="14.0" layoutY="94.0" prefWidth="150.0" />
      <ComboBox fx:id="toLang" layoutX="458.0" layoutY="94.0" prefWidth="150.0" />
      <Button fx:id="btnAudio" layoutX="283.0" layoutY="172.0" mnemonicParsing="false" onAction="#voice" text="Audio" />
   </children>
</AnchorPane>
